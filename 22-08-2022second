column sum
#include<stdio.h>
int main()
{
	int a[100][100],i,j,r,c,sum=0;
	printf("enter number of rows\n");
	scanf("%d",&r);
	printf("enter number of columns\n");
	scanf("%d",&c);
	printf(" enter elements into the array\n");
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
	{
	
		scanf("%d",&a[i][j]);			
	}
}

	for(i=0;i<r;i++)
	{
	printf("enter column number\n");
		scanf("%d",&j);	
	
		for(j;j<c;j++)
		{
			sum=sum+a[j][i]	;
		}
		printf("sum =%d",sum);
		break;
	}
	

}
