#include<stdio.h>
int main()
{
    char s[10];
    scanf("%s",s);
    int i,cupper=0,clower=0,cnumber=0;
    for(i=0;s[i]!='\0';i++)
    {
        if(isupper(s[i]))
        {
           cupper++; 
        }
    if(islower(s[i]))
    {
        clower++;
        
    }
    if(isdigit(s[i]))
    {
        cnumber++;
    }
    }
    
    printf("upper characters=%d",cupper);
    printf("lower characters=%d",clower);
    printf("numbers=%d",cnumber);
}
