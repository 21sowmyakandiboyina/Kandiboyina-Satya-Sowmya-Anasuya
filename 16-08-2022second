
#include<stdio.h>
int main()
{
	int n,a[10],i,j=2,flag;
	printf("enter number of elements");
	scanf("%d",&n);
	printf("enter the elements into the array");
	for(i=0;i<n;i++)
	
	scanf("%d",&a[i]);

printf("the prime number in the array");
	for(i=n;i>=0;i--)
	{
		flag=0;
		for(j=2;j<a[i];j++)
		{
			if(a[i]%j==0)
			flag=1;
			break;
		}
		if(flag==0)
	printf("%d\n",a[i]);
		
	}
return 0;
}
	
