\\row sum\\
#include<stdio.h>
int main()
{
	int i,j,r,c,a[100][100],sum=0;
	printf("enter number of rows :");
	scanf("%d",&r);
	printf("enter number of columns :");
	scanf("%d",&c);
	printf("enter elements into the array :\n");
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}

			printf("enter row number :\n");
	scanf("%d",&i);
	
	for(i;i<r;i++)
	{
		for(j=0;j<c;j++)
		
		sum=sum+a[i][j];
		printf("sum=%d",sum);
		break;
	}
		

}
