1)//write a c program to read a number n.Read x,y values and replace xvalue with y value.//
#include<stdio.h>
int main()
{
	int n,x,y,temp,r,i,count=1;
	printf("enter a number");
	scanf("%d",&n);
scanf("%d",&x);
scanf("%d",&y);
while(n!=0)
{
	r=n%10;
	i=i+(r*count);
	count++;
	if(r==x)
	x=temp;
	y=x;
	temp=y;
	n=n/10;
	
}
printf("%d",i);
	
}
